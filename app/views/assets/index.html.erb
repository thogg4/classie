<% title "Assets" %>

<table>
  <tr>
    <th>Ad</th>
    <th>Image</th>
  </tr>
  <% for asset in @assets %>
    <tr>
      <td><%=h asset.ad_id %></td>
      <td><%=h asset.image %></td>
      <td><%= link_to "Show", asset %></td>
      <td><%= link_to "Edit", edit_asset_path(asset) %></td>
      <td><%= link_to "Destroy", asset, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Asset", new_asset_path %></p>
